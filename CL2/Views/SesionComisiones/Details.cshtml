@model CL2.Models.SesionComision

@{
    ViewData["Title"] = "Detalle Sesión de Comisión";
}


<html lang="en">


<body class="sb-nav-fixed">
    <main>
        <div class="container-fluid px-4">

            <h1 class="mt-lg-4">Detalles Sesión de Comisión</h1>

            @* Breadcrumb *@

            <ol class="breadcrumb mb-4">
                <li class="breadcrumb-item"><a asp-area="" asp-controller="Home" asp-action="Index">Inicio</a></li>
                <li class="breadcrumb-item"><a asp-area="" asp-controller="SesionComisiones" asp-action="Index">Listado de Sesiones</a></li>
                <li class="breadcrumb-item active">Detalle Sesión Comisión</li>

            </ol>

            <hr />
            <dl class="row">
                <dt class="col-sm-2">
                    @Html.DisplayNameFor(model => model.NumeroSesionComision)
                </dt>
                <dd class="col-sm-10">
                    @Html.DisplayFor(model => model.NumeroSesionComision)
                </dd>
                <dt class="col-sm-2">
                    @Html.DisplayNameFor(model => model.PeriodoSesionComision)
                </dt>
                <dd class="col-sm-10">
                    @Html.DisplayFor(model => model.PeriodoSesionComision)
                </dd>
                <dt class="col-sm-2">
                    @Html.DisplayNameFor(model => model.FechaSesionComision)
                </dt>
                <dd class="col-sm-10">
                    @Html.DisplayFor(model => model.FechaSesionComision)
                </dd>
                <dt class="col-sm-2">
                    @Html.DisplayNameFor(model => model.TipoSesionComision)
                </dt>
                <dd class="col-sm-10">
                    @Html.DisplayFor(model => model.TipoSesionComision)
                </dd>
                <dt class="col-sm-2">
                    @Html.DisplayNameFor(model => model.ComisionLegislativos)
                </dt>
                <dd class="col-sm-10">
                    @Html.DisplayFor(model => model.ComisionLegislativos.ComisionLegislativoID)
                </dd>
            </dl>
        </div>
        <div class="btn-group" role="group">
            <a asp-action="Index" class="btn btn-dark">Volver</a>
            <a asp-action="Edit" class="btn btn-warning">Editar</a>
        </div>
    </main>

</body>

 </html>

<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" crossorigin="anonymous"></script>
<script src="js/scripts.js"></script>
