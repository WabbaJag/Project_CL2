@model CL2.Models.SesionComision

@{
    ViewData["Title"] = "Eliminar Sesión de Comisión";
}

<partial name="_NotificationPartial" />


<body class="sb-nav-fixed">
    <h1 class="mt-4">¿Desea eliminar esta Sesión de Comisión?</h1>
    <div class="mt-lg-3">



        <hr />

        @* Breadcrumb *@
        <ol class="breadcrumb mb-4">
            <li class="breadcrumb-item">
                <a asp-area="" asp-controller="Home" asp-action="Index">Inicio</a>
            </li>
            <li class="breadcrumb-item">
                <a asp-area="" asp-controller="SesionComisiones" asp-action="Index">Listado de Sesiones</a>
            </li>
            <li class="breadcrumb-item active">Eliminar Sesión de Comisión</li>
        </ol>

        <hr />


        <div>

            <hr />
            <dl class="row">
                <dt class="col-sm-2">
                    @Html.DisplayNameFor(model => model.ComisionLegislativos)
                </dt>
                <dd class="col-sm-10">
                    @Html.DisplayFor(model => model.ComisionLegislativos.Comision.NombreComision):
                    @Html.DisplayFor(model => model.ComisionLegislativos.Legislativo.AnnoLegislativo)
                </dd>
                <dt class="col-sm-2">
                    @Html.DisplayNameFor(model => model.NumeroSesionComision)
                </dt>
                <dd class="col-sm-10">
                    @Html.DisplayFor(model => model.NumeroSesionComision)
                </dd>
                <dt class="col-sm-2">
                    @Html.DisplayNameFor(model => model.PeriodoSesionComision)
                </dt>
                <dd class="col-sm-10">
                    @Html.DisplayFor(model => model.PeriodoSesionComision)
                </dd>
                <dt class="col-sm-2">
                    @Html.DisplayNameFor(model => model.FechaSesionComision)
                </dt>
                <dd class="col-sm-10">
                    @Html.DisplayFor(model => model.FechaSesionComision)
                </dd>
                <dt class="col-sm-2">
                    @Html.DisplayNameFor(model => model.TipoSesionComision)
                </dt>
                <dd class="col-sm-10">
                    @Html.DisplayFor(model => model.TipoSesionComision)
                </dd>
            </dl>

        

        <form asp-action="Delete">
            <input type="hidden" asp-for="SesionComisionID" />
            <div class="btn-group" role="group">
                <a asp-action="Index" class="btn btn-dark">Volver</a>
                <input type="submit" value="Eliminar" class="btn btn-danger" />
            </div>

        </form>
    </div>

   
</body>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" crossorigin="anonymous"></script>
    <script src="js/scripts.js"></script>

